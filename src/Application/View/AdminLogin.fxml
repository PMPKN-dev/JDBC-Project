<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Text?>

<AnchorPane prefHeight="400.0" prefWidth="600.0" xmlns="http://javafx.com/javafx/8.0.171" xmlns:fx="http://javafx.com/fxml/1" fx:controller="Application.Controller.AdminLogin">
   <children>
      <TextField fx:id="userTextfield" layoutX="88.0" layoutY="77.0" promptText="user" />
      <TextField fx:id="passTextfield" layoutX="88.0" layoutY="144.0" promptText="pass" />
      <Text layoutX="24.0" layoutY="98.0" strokeType="OUTSIDE" strokeWidth="0.0" text="User" />
      <Text layoutX="25.0" layoutY="160.0" strokeType="OUTSIDE" strokeWidth="0.0" text="Pass" />
      <Button layoutX="481.0" layoutY="319.0" mnemonicParsing="false" onAction="#onReturnClick" text="Return" />
      <TextField fx:id="adminResultView" layoutX="88.0" layoutY="251.0" prefHeight="31.0" prefWidth="371.0" />
      <Button fx:id="buttonLogin" layoutX="358.0" layoutY="77.0" mnemonicParsing="false" onAction="#loginAsAdmin" text="Login" />
      <Button fx:id="proceedButton" layoutX="135.0" layoutY="319.0" mnemonicParsing="false" mouseTransparent="true" onAction="#onProceedClicked" opacity="0.0" text="Proceed" />
   </children>
</AnchorPane>
